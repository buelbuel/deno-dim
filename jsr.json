{
    "$schema": "https://jsr.io/schema/config-file.v1.json",
    "name": "@buelbuel/dim",
    "version": "0.1.7",
    "description": "An idiomatic web component helper library for Deno with zero dependencies",
    "exports": {
        ".": "./mod.ts",
        "./base": "./src/base/mod.ts",
        "./utils": "./src/utils/mod.ts"
    },
    "publish": {
        "include": [
            "mod.ts",
            "src/**/*.ts",
            "LICENSE",
            "README.md"
        ],
        "exclude": []
    },
    "readme": "./README.md"
}
